#!/bin/sh
#
# https://support.apple.com/en-gb/guide/keychain-access
# ------------------------------------------------------------------------------
set -e

openssl req \
  -nodes -x509 -sha256 \
  -newkey rsa:2048 \
  -keyout localhost.key \
  -out localhost.crt \
  -subj "/C=GB/ST=London/L=London/O=DfE/OU=EYFS/CN=example.com"
