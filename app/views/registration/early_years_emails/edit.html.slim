= render 'user/debug'

- content_for :page_title do
  = html_title t('early_years_emails.heading')

= govuk_back_link href: edit_registration_training_emails_path

.govuk-grid-row
  .govuk-grid-column-full
    = form_for form, url: registration_early_years_emails_path, method: :patch do |f|
      = f.govuk_error_summary

      = f.opt_in_out :early_years_emails

      div class='govuk-!-margin-top-4'
        = f.govuk_submit t('links.continue')
